<Window x:Class="FLT_dir2webp_cs.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FLT_dir2webp_cs"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="450">
    <Grid Margin="0">
        <Grid.RowDefinitions>
            <RowDefinition Height="48"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <TextBox Margin="62,10,10,10" Text="TextBox" TextWrapping="Wrap" VerticalContentAlignment="Center"/>
            <Button Content="Browse" HorizontalAlignment="Left" Margin="10,10,0,10" Width="47" Click="Button_Click"/>
        </Grid>
    </Grid>
</Window>
